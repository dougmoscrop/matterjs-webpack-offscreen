{
  "name": "matterjs-webpack-offscreen",
  "version": "1.0.0",
  "description": "Experimenting with matter-js and offscreen/worker rendering",
  "scripts": {
    "postinstall": "rimraf node_modules/comlink-loader/node_modules/comlinkjs",
    "build": "webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougmoscrop/matterjs-webpack-offscreen.git"
  },
  "keywords": [
    "matterjs"
  ],
  "author": "Doug Moscrop <doug.moscrop@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougmoscrop/matterjs-webpack-offscreen/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "babel": {
    "presets": [
      "es2017"
    ]
  },
  "homepage": "https://github.com/dougmoscrop/matterjs-webpack-offscreen#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2017": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "worker-plugin": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "keypress.js": "^2.1.5",
    "matter-js": "^0.14.2",
    "comlink": "^4.1.0"
  }
}
